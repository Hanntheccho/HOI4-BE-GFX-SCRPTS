using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CountryList : MonoBehaviour
{
    // List of all countries
    public List<Country> allCountries;

    // Default country to return if needed
    public Country defaultCountry;

    public CountryList()
    {
        allCountries = new List<Country>();
    }

    void Awake()
    {
        InitializeCountries();
        defaultCountry = GetCountryByTag("CHI");
    }

    private int CalculatePowerProjection(Country country)
    {
        // Find the gameObject by name
        GameObject countryGameObject = GameObject.Find(country.countryTag);

        if (countryGameObject == null)
        {
            return 0;
        }

        // Get the renderer component of the GameObject
        Renderer renderer = countryGameObject.GetComponent<Renderer>();

        if (renderer == null)
        {
            return 0;
        }

        // Get the size of the object by multiplying width and height
        float width = renderer.bounds.size.x;
        float height = renderer.bounds.size.y;
        float size = width * height;

        // Convert the size to an integer for power projection, then divide by 1000
        int powerProjection = Mathf.FloorToInt(size / 1000);

        // Assign the calculated power projection to the country
        country.countryPowerProjection = powerProjection;

        return powerProjection;
    }

