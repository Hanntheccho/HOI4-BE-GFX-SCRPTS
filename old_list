        {
            CalculatePowerProjection(country);
        }
    }

    // Method to get a Country instance by tag
    public Country GetCountryByTag(string countryTag)
    {
        // Find the country with the specified tag
        return allCountries.Find(country => country.countryTag == countryTag);
    }
}
